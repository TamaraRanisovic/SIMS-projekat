<Window x:Class="InitialProject.View.ShowRenovationsCancel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="ShowRenovationsCancel" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="renovationsGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="148,39,209,178"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="312,317,0,0" VerticalAlignment="Top" Height="56" Width="114" Click="Button_Click"/>
        <Button Grid.Row="0" Grid.Column="0" Content="BACK" Width="120" Height="75" Margin="10" 
                HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="24" Click="BackButton_Click" />

    </Grid>
</Window>
